<!-- This file doesn't 'do' anything, it some test PLugins for any instrument to use to test the XML assembler -->

<!-- ............................................................................... -->
<!-- Test Secondary Plugin 2 -->

<Plugin>
    <Identifier>SecondaryTest2</Identifier>
    <Provider>SecondaryPlugin</Provider>
    <Name>SecondaryTest2</Name>
    <Description>The Secondary Test Module 2</Description>
    <ResourceKey>SecondaryTest2</ResourceKey>

    <!-- This will never be executed -->
    <CommandCodeBase>92</CommandCodeBase>

    <Command>
        <Identifier>testPlugin2</Identifier>
        <CommandCode>00</CommandCode>
        <CommandVariant>0000</CommandVariant>
        <Description>Tests the Plugin</Description>
        <SendToPort>false</SendToPort>
        <Response>
            <Name>Ack</Name>
            <DataTypeName>String</DataTypeName>
            <Units>Dimensionless</Units>
            <Regex>Ok</Regex>
        </Response>
    </Command>
</Plugin>

