            <!-- Instrument (Primary Plugins) -->
            <!-- ............................................................................... -->






            <!-- ............................................................................... -->
            <!-- Importer Plugin -->

            <Plugin>
                <!-- A virtual Plugin -->
                <Identifier>Importer</Identifier>
                <Name>Importer</Name>
                <SerialNumber>0001</SerialNumber>
                <Description>The Importer Module</Description>
                <ResourceKey>Importer</ResourceKey>

                <CommandCodeBase>80</CommandCodeBase>

                <Command>
                   <Identifier>importRawDataLocal</Identifier>
                   <CommandCode>00</CommandCode>
                   <Software>
                       <Identifier>Importer Command</Identifier>
                       <CollectionID>00-00</CollectionID>
                       <Description>Get a single data file from the local file system</Description>
                   </Software>
                    <Parameter>
                        <Name>Filename</Name>
                        <InputDataType>
                            <DataTypeName>String</DataTypeName>
                        </InputDataType>
                        <TrafficDataType>
                            <DataTypeName>String</DataTypeName>
                        </TrafficDataType>
                        <Units>Dimensionless</Units>
                        <!-- ToDo Regex -->
                        <Tooltip>The filename to be processed</Tooltip>
                    </Parameter>
                    <Parameter>
                        <Name>Format</Name>
                        <InputDataType>
                            <DataTypeName>String</DataTypeName>
                        </InputDataType>
                        <TrafficDataType>
                            <DataTypeName>String</DataTypeName>
                        </TrafficDataType>
                        <Units>Dimensionless</Units>
                        <Choices>FormattedStardata, CompressedStardata</Choices>
                        <Tooltip>Allowed values: Stardata (XML)</Tooltip>
                    </Parameter>
                    <Parameter>
                        <Name>Filter</Name>
                        <InputDataType>
                            <DataTypeName>String</DataTypeName>
                        </InputDataType>
                        <TrafficDataType>
                            <DataTypeName>String</DataTypeName>
                        </TrafficDataType>
                        <Units>Dimensionless</Units>
                        <Choices>PassThrough</Choices>
                        <Tooltip>Allowed values: PassThrough (no filtering)</Tooltip>
                    </Parameter>
                    <SendToPort>false</SendToPort>
                    <Response>
                        <Name>Ack</Name>
                        <DataTypeName>String</DataTypeName>
                        <Units>Dimensionless</Units>
                        <Regex>Ok</Regex>
                    </Response>
               </Command>
            </Plugin>

